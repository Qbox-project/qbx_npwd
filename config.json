{
    "PhoneAsItem": {
      "enabled": true,
      "exportResource": "qbx_npwd",
      "exportFunction": "HasPhone"
    },
    "general": {
     "useResourceIntegration": true,
      "toggleKey": "f1",
      "toggleCommand": "phone",
      "defaultLanguage": "en"
    },
    "contacts": {
      "frameworkPay": false,
      "payResource": "my-core-resource",
      "payFunction": "myCheckerFunction"
    },
    "database": {
      "useIdentifierPrefix": true,
      "playerTable": "players",
      "identifierColumn": "citizenid",
      "identifierType": "license",
      "profileQueries": true,
      "phoneNumberColumn": "phone_number"
    },
    "profanityFilter": {
      "enabled": true,
      "badWords": ["esx", "qbus"]
    },
    "images": {
      "url": "https://api.fivemanage.com/api/image",
      "type": "image",
      "imageEncoding": "webp",
      "contentType": "multipart/form-data",
      "useContentType": false,
      "useWebhook": false, //true = using webhook on discord, for example -- found on PE discord (https://projecterror.dev/docs/npwd/start/installation/#discord)
      "authorizationHeader": "Authorization",
      "authorizationPrefix": "",
      "useAuthorization": true,
      "returnedDataIndexes": ["url"]
    },
    "debug": {
      "level": "error",
      "enabled": true,
      "sentryEnabled": true
    },
    "defaultContacts": [],
    "disabledApps": [ "BROWSER" ],
    "apps": [ "npwd_qbx_mail", "npwd_qbx_garage" ],
    "voiceMessage": {
      "enabled": true,
      "authorizationHeader": "Authorization",
      "url": "https://api.fivemanage.com/api/audio",
      "returnedDataIndexes": ["url"]
    }
  }